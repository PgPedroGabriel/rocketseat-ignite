{
    "env": {
        "es6": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "standard"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
